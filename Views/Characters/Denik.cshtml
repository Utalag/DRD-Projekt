                                            <th>Název</th>
                                            <th>Úč</th>
                                            <th>út</th>
                                            <th colspan="3">Dostřel</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{
                                            foreach(var item in ViewBag.WeaponsList)
                                            {
                                                if(item.Weapon.ClassWeapon == WeaponClassEnum.firearm || item.Weapon.ClassWeapon == WeaponClassEnum.throwing)
